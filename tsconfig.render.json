{
  "compilerOptions": {
    "outDir": "./dist/render",
    "types": [
      "node"
    ],
    "target": "es2016",
    "lib": ["ES2021"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["render/**/*.ts"]
}
